{
  "name": "jotsy",
  "version": "1.1.4",
  "author": "Eric Morrison",
  "description": "Minimalist, colorful log outputs.",
  "license": "MIT",
  "keywords": [
    "ts",
    "log",
    "logger",
    "logging",
    "typescript",
    "formatting",
    "terminal",
    "console",
    "print"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/55Cancri/jotsy.git"
  },
  "bugs": {
    "url": "https://github.com/55Cancri/jotsy/issues"
  },
  "homepage": "https://github.com/55Cancri/jotsy#readme",
  "scripts": {
    "start": "npx nodemon",
    "build": "npx rimraf ./dist && npx tsc",
    "test": "jest --coverage --config jestconfig.json",
    "test-watch": "jest --coverage --watch --config jestconfig.json",
    "demo": "npx ts-node --files ./experiment/example.ts --project ./tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "yarn test"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "date-fns": "^2.10.0",
    "husky": "^4.2.3",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.0",
    "@types/test-console": "^1.1.0",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "test-console": "^1.1.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
